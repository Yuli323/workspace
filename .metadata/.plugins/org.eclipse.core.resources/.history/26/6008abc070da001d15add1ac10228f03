package project2__2;

public class Main {

    public static void main(String[] args) {
        DeckOfCard deckOfCard ;
        deckOfCard = new DeckOfCard();
        
        //第一次
        deckOfCard.shuffle();
        deckOfCard.dealCard(3);
        
        //第二次
        deckOfCard.shuffle();
        deckOfCard.dealCard(2);
        
        //第三次
        deckOfCard.shuffle();
        deckOfCard.dealCard(3);
    }
}
